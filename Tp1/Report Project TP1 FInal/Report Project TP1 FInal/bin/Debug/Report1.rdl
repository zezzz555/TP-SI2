      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT dimCliente.*, dimAlbum.*, TabelaFactos.NumPistas, TabelaFactos.ValorVenda
FROM     dimCliente INNER JOIN
                  TabelaFactos ON dimCliente.IdCliente = TabelaFactos.IdCliente INNER JOIN
                  dimAlbum ON TabelaFactos.IdAlbum = dimAlbum.IdAlbum</CommandText>
      </Query>
