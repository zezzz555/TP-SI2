  <Kpis>
    <Kpi dwd:design-time-name="1cb64482-4b60-4d86-b3a8-095e6f94bdbb">
      <ID>KPI</ID>
      <Name>KPI_VendaPorMes</Name>
      <AssociatedMeasureGroupID>Tabela Factos</AssociatedMeasureGroupID>
      <Value>[Measures].[Num Pistas]</Value>
      <Goal>Case
When [dimData].[Month]
IS [dimData].[Month].[January 2009]
then 36
When [dimData].[Month]
IS [dimData].[Month].[September 2010]
then 37
When [dimData].[Month]
IS [dimData].[Month].[November 2011]
then 24
When [dimData].[Month]
IS [dimData].[Month].[September 2012]
then 29
When [dimData].[Month]
IS [dimData].[Month].[February 2013]
then 28
When [dimData].[Month]
IS [dimData].[Month].[April 2013]
then 34
else 38
end
</Goal>
      <Status>Case
When
KPIValue("KPI_VendaPorMes")/KPIGoal("KPI_VendaPorMes")&gt;=.9
then 1
When KPIValue("KPI_VendaPorMes")/KPIGoal("KPI_VendaPorMes")&lt;.9
AND KPIValue("KPI_VendaPorMes")/KPIGoal("KPI_VendaPorMes")&gt;=.5
then 0
else -1
End</Status>
      <Trend>Case
When IsEmpty ([dimData].[Month].PrevMember) then 0
When ([dimData].[Month],[Measures].[Num Pistas]) &gt;
([dimData].[Month].PrevMember,[Measures].[Num Pistas]) then 1
When ([dimData].[Month],[Measures].[Num Pistas]) =
([dimData].[Month].PrevMember,[Measures].[Num Pistas]) then 0
When ([dimData].[Month],[Measures].[Num Pistas]) &lt;
([dimData].[Month].PrevMember,[Measures].[Num Pistas]) then -1
End</Trend>
      <TrendGraphic>Standard Arrow</TrendGraphic>
      <StatusGraphic>Gauge - Ascending</StatusGraphic>
    </Kpi>
  </Kpis>
